{%- comment -%}
Composite test: for loop with conditional
Loop iteration with condition check per item
{%- endcomment -%}
{% assign product = products | first %}
{% for variant in product.variants %}
  {% if variant.available %}
    {% if variant.price > 100 %}
<div class="premium">{{ variant.title }}: {{ variant.price }}</div>
    {% else %}
<div class="regular">{{ variant.title }}: {{ variant.price }}</div>
    {% endif %}
  {% endif %}
{% endfor %}
