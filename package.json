{
  "name": "liquid-engine-benchmarks",
  "type": "module",
  "scripts": {
    "format": "biome format --write .",
    "format:check": "biome format .",
    "lint": "biome lint .",
    "lint:fix": "biome lint --write .",
    "lint:typecheck": "eslint .",
    "check": "biome check --write . && eslint .",
    "generate:schema": "bun scripts/generate-schema.ts",
    "validate:bench": "bun scripts/validate-schema.ts schema/bench.schema.json bench.json",
    "validate:result": "bun scripts/validate-schema.ts schema/result.schema.json"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.8",
    "@faker-js/faker": "^10.1.0",
    "@typescript-eslint/eslint-plugin": "^8.48.0",
    "@typescript-eslint/parser": "^8.48.0",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "bun-types": "^1.3.3",
    "eslint": "^9.39.1",
    "ts-json-schema-generator": "^2.4.0"
  }
}
